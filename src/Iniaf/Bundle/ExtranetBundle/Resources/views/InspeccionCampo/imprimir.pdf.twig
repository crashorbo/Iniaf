                <td padding-left="10">{{ entity.registroCampo.fechaFloracion | date('d--m-Y') }}</td>
            </tr>
            <tr>
                <td padding-left="10"><b>Fecha de Cosecha: </b></td>
                <td padding-left="10">{{ entity.registroCampo.fechaCosecha | date('d-m-Y') }}</td>
            </tr>
            <tr>
                <td padding-left="10"><b>Aislamiento: </b></td>
                <td padding-left="10">{{ entity.aislamiento }}</td>
            </tr>
            <tr>
                <td padding-left="10"><b>Poblacion por Ha: </b></td>
                <td padding-left="10">{{ entity.poblacionHectarea }}</td>
            </tr>
