                <td padding-left="10">{{ entity.registroCampo.variedad.nombre }}</td>
            </tr>
            <tr>
                <td padding-left="10"><b>Generacion Utilizada: </b></td>
                <td padding-left="10">{{ entity.generacionUtilizada.nombre }}</td>
            </tr>
            <tr>
                <td padding-left="10"><b>Generacion Pretendida: </b></td>
                <td padding-left="10">{{ entity.generacionPretendida.nombre }}</td>
            </tr>
            <tr>
                <td padding-left="10"><b>Cultivo Anterior: </b></td>
                <td padding-left="10">{{ entity.registroCampo.cultivoAnterior }}</td>
            </tr>
